{
  "expo": {
    "name": "Masa Home",
    "slug": "massage-home-spa",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "masa-spa",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "androidNavigationBar": {
      "visible": "immersive"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Masa Home cần truy cập thư viện ảnh để bạn có thể chọn và tải lên ảnh đại diện cá nhân, cũng như cung cấp các hình ảnh hồ sơ chuyên môn cần thiết khi thực hiện đăng ký trở thành kỹ thuật viên của hệ thống.",
        "NSCameraUsageDescription": "Masa Home cần sử dụng Camera để bạn có thể chụp ảnh đại diện trực tiếp hoặc chụp ảnh các giấy tờ, bằng cấp liên quan để hoàn tất hồ sơ đăng ký làm kỹ thuật viên ngay trên ứng dụng.",
        "NSPhotoLibraryAddUsageDescription": "Masa Home cần quyền lưu ảnh vào thư viện để bạn có thể lưu lại các thông tin xác nhận đăng ký hoặc hình ảnh từ dịch vụ.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.nhmsoftbujhuyanh.massagehomespa"
    },
    "android": {
      "versionCode": 5,
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.nhmsoftbujhuyanh.massagehomespa",
      "googleServicesFile": "./google-services.json",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDc5gJqICVzNvMKQrs7UEgRBpVF89le6lo"
        }
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.SCHEDULE_EXACT_ALARM",
        "android.permission.CAMERA"
      ]
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-localization",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo_title.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d8979fe6-2cb0-46ef-a572-03782064d295"
      }
    },
    "owner": "nhmsoftbujhuyanh"
  }
}
